<%- include('header')%>

<h1>Registrate:</h1>
<form action="/home/registro" method="POST">
    <label for="name">NOMBRE DE USUARIO:</label>
    <input name='name' type="text" placeholder="Inserte su nombre aqui">
    <br>
    <label for="email">EMAIL:</label>
    <input name='email' type="email"placeholder="Inserte su email aqui">
    <br>
    <label for="">PASSWORD:</label>
    <input name='password' type="text" placeholder="Inserte su password aqui">
    <br>
    <label for="birthday">FECHA DE NACIMIENTO:</label>
    <input name='birthday' type="date">
    <br>
    <button type="submit">ENVIAR</button>
</form>

<%- include('footer')%>